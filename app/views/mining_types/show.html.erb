<p id="notice"><%= notice %></p>



<h1><%= I18n.t("showing.mining_type") %></h1>

<form>
  <div class="form-group">
    <%= label_tag @mining_type.class.human_attribute_name(:description) %>
    <%= text_field_tag nil, @mining_type.description, class:"form-control", disabled: true %>
  </div>

  <div class="form-group">
    <%= label_tag @mining_type.class.human_attribute_name(:acronym) %>
    <%= text_field_tag nil,@mining_type.acronym, class:"form-control", disabled: true %>
  </div>

</form>

<br>

<%= link_to 'Edit', edit_coin_path(@mining_type), class:"btn btn-primary" %> 
<%= link_to I18n.t("links.back"), mining_types_path, class:"btn btn-primary" %>






